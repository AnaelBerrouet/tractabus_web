<h1>Show Article</h1>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @article.title %>
  </li>

  <li>
    <strong>Pagerank:</strong>
    <%= @article.pagerank %>
  </li>

  <li>
    <strong>Readability:</strong>
    <%= @article.readability %>
  </li>

  <li>
    <strong>Word count:</strong>
    <%= @article.word_count %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.article_path(@conn, :edit, @article) %></span>
<span><%= link "Back", to: Routes.article_path(@conn, :index) %></span>
